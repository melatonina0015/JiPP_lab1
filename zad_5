#include <iostream>     

using namespace std;

int main () 
{
    int matrix_1[2][3] = {5, -6, 3, 10, 14, -7};
    int matrix_2[2][3] = {-3, -12, 7, -2, 9, 16};

    for(int i = 0; i < 2; i++)
    {
        for(int j = 0; j < 3; j++)
    
        {
            matrix_2[i][j] += matrix_1[i][j];
            cout << matrix_2[i][j] << " ";
        }
        cout << endl;
    }

    system("pause");
    return 0;
}
